<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<GridPane alignment="center" hgap="10" vgap="10"
          xmlns="http://javafx.com/javafx/8.0.121"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Pane prefHeight="565.0" prefWidth="683.0"
              GridPane.rowIndex="2">
            <children>
                <Label fx:id="disptemp" layoutX="87.0" layoutY="191.0"
                       prefHeight="46.0" prefWidth="228.0" textFill="#51ff00">
                    <font>
                        <Font name="Arial Nova Italic" size="20.0" />
                    </font></Label>
                <Rectangle fx:id="coolindi" arcHeight="5.0"
                           arcWidth="5.0" fill="WHITE" height="46.0" layoutX="47.0" layoutY="191.0"
                           stroke="BLACK" strokeType="INSIDE" width="24.0" />
                <Rectangle fx:id="normindi" arcHeight="5.0"
                           arcWidth="5.0" fill="WHITE" height="46.0" layoutX="47.0" layoutY="266.0"
                           stroke="BLACK" strokeType="INSIDE" width="24.0" />
                <Rectangle fx:id="hotindi" arcHeight="5.0"
                           arcWidth="5.0" fill="WHITE" height="46.0" layoutX="47.0" layoutY="338.0"
                           stroke="BLACK" strokeType="INSIDE" width="24.0" />
                <Label fx:id="disptemp2" layoutX="87.0" layoutY="266.0"
                       prefHeight="46.0" prefWidth="228.0" textFill="#5a15c9" />
                <Label fx:id="disptemp3" layoutX="87.0" layoutY="338.0"
                       prefHeight="46.0" prefWidth="228.0" textFill="RED" />
                <Slider fx:id="slide" layoutX="47.0" layoutY="102.0"
                        majorTickUnit="20.0" minorTickCount="10"
                        onDragDetected="#identifyslidetemp" prefHeight="51.0" prefWidth="213.0"
                        showTickLabels="true" showTickMarks="true" />
                <Label layoutX="50.0" layoutY="40.0" prefHeight="30.0"
                       prefWidth="354.0" text="Temperature controller in Piciculture">
                    <font>
                        <Font name="System Bold" size="20.0" />
                    </font></Label>
            </children></Pane>
    </children>
